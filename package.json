{
  "name": "@js.properties/parser",
  "version": "0.0.1",
  "description": "JavaScript .properties parser",
  "keywords": [
    ".properties",
    "properties",
    "parser"
  ],
  "repository": "jsproperties/parser",
  "homepage": "https://github.com/jsproperties/parser",
  "bugs": "https://github.com/jsproperties/parser/issues",
  "author": "pallxk <github@pallxk.com>",
  "main": "lib/index.js",
  "scripts": {
    "test": "tap test/js/parse.js test/js/namespace.js",
    "prepare": "yon run pegjs && yon run babel && yon run webpack",
    "watch": "yon run pegjs:watch & yon run babel:watch & yon run webpack:watch",
    "pegjs": "pegjs -o src/properties.pegjs.js src/properties.pegjs",
    "pegjs:watch": "npm-watch pegjs",
    "babel": "babel -d lib src",
    "babel:watch": "babel -w -d lib src",
    "webpack": "webpack --config webpack.dev.js && webpack --config webpack.prod.js",
    "webpack:watch": "webpack -w --config webpack.dev.js"
  },
  "watch": {
    "pegjs": "src/properties.pegjs"
  },
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-es2015-arrow-functions": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "lodash": "^4.17.4",
    "npm-watch": "^0.3.0",
    "pegjs": "^0.10.0",
    "pegjs-loader": "^0.5.4",
    "tap": "^11.0.0",
    "uglifyjs-webpack-plugin": "^1.1.4",
    "webpack": "^3.10.0",
    "webpack-merge": "^4.1.1",
    "yarn-or-npm": "^2.0.4"
  }
}
